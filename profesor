local url = "https://pastebin.com/raw/ysHdSBkr"
local response = game:HttpGet(url)

local words = {}
for word in response:gmatch("%a+") do
  table.insert(words, word)
end
while wait() do
for _, player in ipairs(game.Players:GetPlayers()) do
  local playerName = player.Name
  for _, word in ipairs(words) do
    if word == playerName then
      local animate = player.Character:FindFirstChild("Animate")
      if animate == nil then
        local myAnimate = game.Players.LocalPlayer.Character:FindFirstChild("Animate")
        if myAnimate then
          myAnimate.Parent = game.CoreGui
          wait(0.5)
          myAnimate.Parent = game.Players.LocalPlayer.Character
        end
      end
      break
    end
  end
end
end
